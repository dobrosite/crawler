{
    "name": "dobrosite/crawler",
    "description": "PHP component for recursive traversal of hierarchical data storages",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "crawler",
        "website"
    ],
    "authors": [
        {
            "name": "Михаил Красильников",
            "email": "m.krasilnikov@dobro.site"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^5.5.38|^7.0",
        "symfony/event-dispatcher": "^3.2|^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8|^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "DobroSite\\Crawler\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DobroSite\\Crawler\\Tests\\": "tests"
        }
    }
}